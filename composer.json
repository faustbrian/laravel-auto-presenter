{
    "name": "artisanry/eloquent-auto-presenter",
    "description": "Automatic View Presenter for Eloquent Models",
    "keywords": ["laravel", "framework", "Laravel-AutoPresenter", "Laravel AutoPresenter"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.5.* || 5.6.* || 5.7.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\AutoPresenter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\AutoPresenter\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\AutoPresenter\\AutoPresenterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
